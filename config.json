{
"plugin": {
    "reference": "iso",
    "type": "SOURCE",
    "filenames": ["iso.py"],
    "localstorage": true
},
"task": {
    "log": -1,
    "trigger": {
      "type": "SCHEDULE",
      "interval": "7 days"
    }
},
"middleware": {
    "modules": [
        { "order": 1, "name": "TimezoneSafeControl", "critical": true, "params": {}},
        { "order": 2, "name": "CutJunkCharactersFromDocumentText", "critical": true, "params": {
            "fields": ["text", "abstract"]
        }},
        { "order": 3, "name": "FilterOnlyNewDocumentWithDB", "critical": true, "params": {}},
        { "order": 4, "name": "SaveDocumentToDB", "critical": true, "params": {}}
    ],
    "bus": {
        "entities": []
    }
},
"payload": {
    "file": "iso.py",
    "class": "ISO",
    "entry": {
        "point": "content",
        "params": [
          {"key": "webdriver", "value": {"type": "module", "name": "WebDriver"}},
          {"key": "max_count_documents", "value": {"type": "const", "value": 200}},
            {"key": "urls", "value": {"type": "const", "value": ["https://www.iso.org/ics/03.060/x/",
                                                                  "https://www.iso.org/ics/35.240.40/x/",
                                                                  "https://www.iso.org/ics/35.240.15/x/",
                                                                  "https://www.iso.org/ics/35.020/x/"]
          ]}}
        ]
    },
    "additional_methods": null
}
}